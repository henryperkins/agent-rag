# Test Environment Configuration
# Minimal configuration for running tests

# Application Settings
PROJECT_NAME=agentic-azure-chat-test
NODE_ENV=test
PORT=8787

# Azure AI Search (Mock values for testing)
AZURE_SEARCH_ENDPOINT=https://mock-search.search.windows.net
AZURE_SEARCH_API_VERSION=2025-08-01-preview
AZURE_SEARCH_DATA_PLANE_API_VERSION=2025-09-01
AZURE_SEARCH_INDEX_NAME=test-index
AZURE_SEARCH_API_KEY=mock-api-key-for-testing
AZURE_KNOWLEDGE_AGENT_NAME=test-agent

# Azure OpenAI (Mock values for testing)
AZURE_OPENAI_ENDPOINT=https://mock-openai.openai.azure.com
AZURE_OPENAI_API_VERSION=v1
AZURE_OPENAI_GPT_DEPLOYMENT=gpt-5
AZURE_OPENAI_GPT_MODEL_NAME=gpt-5
AZURE_OPENAI_API_KEY=mock-openai-key

# Embedding
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-3-large
AZURE_OPENAI_EMBEDDING_ENDPOINT=https://mock-embedding.cognitiveservices.azure.com
AZURE_OPENAI_EMBEDDING_API_KEY=mock-embedding-key

# Google Custom Search (Optional)
GOOGLE_SEARCH_API_KEY=
GOOGLE_SEARCH_ENGINE_ID=
GOOGLE_SEARCH_ENDPOINT=https://customsearch.googleapis.com/customsearch/v1

# Retrieval Settings
RAG_TOP_K=5
RERANKER_THRESHOLD=2.5
RETRIEVAL_FALLBACK_RERANKER_THRESHOLD=1.5
MAX_DOCS_FOR_RERANKER=100
TARGET_INDEX_MAX_DOCUMENTS=100
RETRIEVAL_MIN_DOCS=3
SEARCH_MIN_COVERAGE=0.8

# Adaptive Retrieval
ENABLE_ADAPTIVE_RETRIEVAL=false
ADAPTIVE_MIN_COVERAGE=0.4
ADAPTIVE_MIN_DIVERSITY=0.3
ADAPTIVE_MAX_ATTEMPTS=3

# Critic Settings
ENABLE_CRITIC=true
CRITIC_MAX_RETRIES=1
CRITIC_THRESHOLD=0.8

# Feature Flags
ENABLE_CITATION_TRACKING=false
ENABLE_WEB_QUALITY_FILTER=false
ENABLE_LAZY_RETRIEVAL=false
ENABLE_INTENT_ROUTING=false
ENABLE_SEMANTIC_SUMMARY=false
ENABLE_CRAG=false
ENABLE_ACADEMIC_SEARCH=false
ENABLE_SEMANTIC_MEMORY=false
ENABLE_QUERY_DECOMPOSITION=false
ENABLE_WEB_RERANKING=false
ENABLE_SEMANTIC_BOOST=false
ENABLE_RESPONSE_STORAGE=false

# Logging
LOG_LEVEL=error
CORS_ORIGIN=http://localhost:5173
